      <% elsif couple.estimated_time == 3 %>
        <img class="icon-challenge-card" src="https://res.cloudinary.com/dvgcwuo68/image/upload/v1709988688/chronometer_3_yujw23.png" alt="Timer">
      <% end %>
    </div>


